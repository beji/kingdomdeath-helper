{
  "name": "staticpage_fragment",
  "version": "1.0.0",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn run dev",
    "dev": "npm-run-all dev:*",
    "dev:lint": "tslint -c tslint.json ./src/hmr.ts",
    "dev:run": "cross-env NODE_ENV=development ts-node ./src/hmr.ts",
    "prod": "cross-env NODE_ENV=production ts-node ./src/hmr.ts",
    "build": "npm-run-all build:*",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.client.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.server.prod.js",
    "format": "npm-run-all format:*",
    "format:hmr": "tsfmt -r ./src/hmr.ts",
    "format:code": "tsfmt -r",
    "test": "mocha -r ts-node/register src/**/*.test.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/express": "^4.11.1",
    "@types/mocha": "^5.2.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.0.0",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.19",
    "@types/redux": "^3.6.0",
    "@types/socket.io": "^1.4.33",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.3",
    "@types/webpack": "^4.1.4",
    "@types/webpack-env": "^1.13.5",
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "mocha": "^5.1.1",
    "npm-run-all": "^4.1.2",
    "stylelint": "^9.2.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.5.0",
    "stylelint-processor-styled-components": "^1.3.1",
    "ts-loader": "^4.1.0",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.3",
    "typescript-formatter": "^7.2.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-stats-plugin": "^0.2.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "express": "^4.16.3",
    "fast-deep-equal": "^1.1.0",
    "morgan": "^1.9.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-hot-loader": "^4.1.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "socket.io": "^2.1.0",
    "styled-components": "^3.2.6",
    "styled-components-ts": "^0.0.14",
    "uuid": "^3.2.1"
  }
}
