{
  "name": "staticpage_fragment",
  "version": "1.0.0",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn run dev",
    "dev": "npm-run-all dev:*",
    "dev:lint": "tslint -c tslint.json ./src/hmr.ts",
    "dev:test": "mocha",
    "dev:run": "cross-env NODE_ENV=development ts-node ./src/hmr.ts",
    "prod": "cross-env NODE_ENV=production node ./src/hmr.js",
    "build": "npm-run-all build:*",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.client.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.server.prod.js",
    "build:hmr": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.hmr.prod.js",
    "format": "npm-run-all format:*",
    "format:hmr": "tsfmt -r ./src/hmr.ts",
    "format:code": "tsfmt -r",
    "test": "nyc mocha",
    "test-only": "mocha"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts",
      "coverage",
      "data",
      "dist",
      "public",
      "webpack"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.3.2",
    "@types/qrcode.react": "^0.8.2",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.3",
    "@types/react-redux": "^7.0.5",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "4.1.8",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.26",
    "@types/webpack-env": "^1.13.9",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "^4.2.0",
    "choma": "^1.2.1",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^12.0.2",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.5.0",
    "stylelint-processor-styled-components": "^1.5.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.14.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.5.1",
    "typescript": "^3.3.4000",
    "typescript-formatter": "^7.2.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-hot-server-middleware": "^0.6.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-stats-plugin": "^0.2.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@reactivex/rxjs": "^6.4.0",
    "express": "^4.16.4",
    "fuse.js": "^3.4.4",
    "morgan": "^1.9.1",
    "qrcode.react": "^0.9.3",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-hot-loader": "^4.3.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "socket.io": "^2.2.0",
    "styled-components": "^4.2.0",
    "uuid": "^3.3.2"
  }
}
