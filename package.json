{
  "name": "staticpage_fragment",
  "version": "1.0.0",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn run dev",
    "dev": "npm-run-all dev:*",
    "dev:lint": "tslint -c tslint.json ./src/hmr.ts",
    "dev:test": "mocha",
    "dev:run": "cross-env NODE_ENV=development ts-node ./src/hmr.ts",
    "prod": "cross-env NODE_ENV=production node ./src/hmr.js",
    "build": "npm-run-all build:*",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.client.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.server.prod.js",
    "build:hmr": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.hmr.prod.js",
    "format": "npm-run-all format:*",
    "format:hmr": "tsfmt -r ./src/hmr.ts",
    "format:code": "tsfmt -r",
    "test": "nyc mocha",
    "test-only": "mocha"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts",
      "coverage",
      "data",
      "dist",
      "public",
      "webpack"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.3.2",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router-dom": "^4.2.7",
    "@types/redux": "^3.6.0",
    "@types/socket.io": "^1.4.34",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.3",
    "@types/webpack": "^4.4.0",
    "@types/webpack-env": "^1.13.5",
    "awesome-typescript-loader": "^5.0.0",
    "chai": "^4.1.2",
    "choma": "^1.2.1",
    "cross-env": "^5.1.6",
    "hard-source-webpack-plugin": "^0.7.6",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^12.0.2",
    "stylelint": "^9.2.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.5.0",
    "stylelint-processor-styled-components": "^1.3.1",
    "ts-loader": "^4.4.1",
    "ts-node": "^6.1.1",
    "tsconfig-paths": "^3.4.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.9.1",
    "typescript-formatter": "^7.2.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-stats-plugin": "^0.2.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@reactivex/rxjs": "^6.2.1",
    "express": "^4.16.3",
    "fuse.js": "^3.2.1",
    "morgan": "^1.9.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^4.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "socket.io": "^2.1.1",
    "styled-components": "^3.3.2",
    "styled-components-ts": "^0.0.14",
    "uuid": "^3.2.1"
  }
}
